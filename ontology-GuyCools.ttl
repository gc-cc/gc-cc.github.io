@prefix gc: <https://gc-cc.github.io/ontology_GuyCools.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://gc-cc.github.io/ontology_GuyCools.ttl> a owl:Ontology ;
    rdfs:label "Guy Cools Ontology" ;
    rdfs:comment "An ontology describing photos of individuals holding the 'C' of Cultuurconnect." .

gc:Photo a owl:Class ;
    rdfs:label "Photo" ;
    rdfs:comment "A photographic representation of a person holding the 'C' of Cultuurconnect." .

gc:Person a owl:Class ;
    rdfs:label "Person" ;
    rdfs:comment "A person represented in the photo." .

gc:Location a owl:Class ;
    rdfs:label "Location" ;
    rdfs:comment "A place where the photo was taken." .

gc:Photographer a owl:Class ;
    rdfs:label "Photographer" ;
    rdfs:comment "The person or company who took the photo." .

gc:isDoctype a rdf:Property ;
    rdfs:label "is doctype" ;
    rdfs:comment "Specifies the type of document (e.g., photo ID)." ;
    rdfs:domain gc:Photo ;
    rdfs:range xsd:string .

gc:title a rdf:Property ;
    rdfs:label "title" ;
    rdfs:comment "The title of the photo." ;
    rdfs:domain gc:Photo ;
    rdfs:range xsd:string .

gc:represents a rdf:Property ;
    rdfs:label "represents" ;
    rdfs:comment "Indicates the person represented in the photo." ;
    rdfs:domain gc:Photo ;
    rdfs:range gc:Person .

gc:hasPrimaryColour a rdf:Property ;
    rdfs:label "has primary colour" ;
    rdfs:comment "Defines the main colour of the Cultuurconnect 'C' in the photo." ;
    rdfs:domain gc:Photo ;
    rdfs:range xsd:string .

gc:wasTakenAt a rdf:Property ;
    rdfs:label "was taken at" ;
    rdfs:comment "Specifies the location where the photo was taken." ;
    rdfs:domain gc:Photo ;
    rdfs:range gc:Location .

gc:wasTakenBy a rdf:Property ;
    rdfs:label "was taken by" ;
    rdfs:comment "Identifies the photographer of the photo." ;
    rdfs:domain gc:Photo ;
    rdfs:range gc:Photographer .

gc:description a rdf:Property ;
    rdfs:label "description" ;
    rdfs:comment "A textual description of the photo." ;
    rdfs:domain gc:Photo ;
    rdfs:range xsd:string .
